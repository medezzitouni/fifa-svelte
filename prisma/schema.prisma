// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Player {
  id         String    @id @default(uuid())
  firstname  String
  lastname   String
  goal       Float
  salary     Float
  devise     String
  pictureURl String
}
